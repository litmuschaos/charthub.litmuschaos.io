{
	"name": "create-react-app-typescript-redux",
	"version": "2.0.0",
	"dependencies": {
		"@material-ui/core": "4.9.0",
		"@material-ui/icons": "4.5.1",
		"@types/js-yaml": "^3.12.5",
		"ace-builds": "^1.4.12",
		"brace": "^0.11.1",
		"history": "^4.10.1",
		"js-yaml": "^3.14.0",
		"localforage": "1.7.3",
		"react": "16.12.0",
		"react-ace": "^9.1.1",
		"react-dom": "16.12.0",
		"react-markdown": "^5.0.0",
		"react-redux": "7.1.3",
		"react-router-dom": "5.1.2",
		"react-router-sitemap": "^1.2.0",
		"react-scripts": "^4.0.0",
		"redux-devtools-extension": "2.13.8",
		"redux-logger": "3.0.6",
		"redux-persist": "6.0.0",
		"redux-thunk": "2.3.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"cy:run": "cypress run",
		"cyrun": "start-server-and-test start http://localhost:3000 cy:run",
		"format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|md|vue)\"",
		"sitemap": "babel-node ./src/sitemap-generator.js && generate-robotstxt --config ./src/robots-txt.config.js ./public/robots.txt",
		"predeploy": "npm run sitemap"
	},
	"lint-staged": {
		"*.+(js|jsx|ts|tsx)": [
			"prettier --write",
			"git add"
		],
		"*.+(json|yml|yaml|md|vue)": [
			"prettier --write"
		],
		"*.+(css|scss)": [
			"prettier --write"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/history": "^4.7.11",
		"@types/jest": "24.0.25",
		"@types/node": "^12.20.55",
		"@types/react": "16.9.19",
		"@types/react-dom": "16.9.5",
		"@types/react-redux": "7.1.7",
		"@types/react-router-dom": "5.1.3",
		"@types/redux-logger": "3.0.7",
		"@types/webpack-env": "1.15.1",
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "^6.26.0",
		"cypress": "^12.16.0",
		"eslint-plugin-react": "7.17.0",
		"generate-robotstxt": "^5.0.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.6",
		"prettier": "^2.0.5",
		"start-server-and-test": "^1.11.0",
		"typescript": "^4.9.5"
	}
}
